<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
    <style type="text/css">
      html { height: 80% }
      body { height: 80%; margin: 5; padding: 10 }
      #map_canvas { height: 100% }
    </style>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDDl0u4l76xuStrTo9PJbCurMXMi23kEjk&sensor=false" type="text/javascript">
    </script>


    <script type="text/javascript">
      var mapKey="AIzaSyDDl0u4l76xuStrTo9PJbCurMXMi23kEjk";
      var sensor=false;
      
     function initialize() {
        var myOptions = {
            center: new google.maps.LatLng(-33.863093, 151.207731),
            zoom: 15,
            mapTypeId: google.maps.MapTypeId.HYBRID
        };
          var map = new google.maps.Map(document.getElementById("map_canvas"),
              myOptions);        
      }

      function getScriptSrc() {
	return "http://maps.googleapis.com/maps/api/js?key=" + mapKey + "&sensor=false&callback=initialize"
      }

      function loadScript() {
         var script = document.createElement("script");
         script.type = "text/javascript";
         script.src = getScriptSrc();
         document.body.appendChild(script);
      }
      window.onload =loadScript
    </script>
  </head>
  <body onload="initialize()">
    <div id="map_canvas" style="width:100%; height:100%"></div>
  </body>
</html>


